module Tests.Test1;

import Standard.IO as _;
import extern "stdio.h";

class HelloWorld
{
public:
	static func Print(output: const)
	{
		Print(output);
		Print("\n");
	}

	static Func = (output: const)
	{
		Print(output);
		Print("\n");
	};

	const abstract Thingy();
};

func main(args: char[][])
{
	HelloWorld.Print("Hello, World!");
	HelloWorld.Func("Hello, World!");

	Print("The first argument is {}\n", args[0]);

	if 5 > 3
	{
		Print("5 is greater than 3");
	}

	return 0;
}
